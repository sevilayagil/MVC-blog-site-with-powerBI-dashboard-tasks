@model  IEnumerable<sitem.Models.Kategori>
@{
    Layout = null;
}
<h4>Kategoriler</h4>
<div class="row">
    <div class="col-lg-6">
        @foreach (var item in Model)
        {
            <ul class="list-unstyled">
                <li>
                    <a href="/Home/KategoriMakale/@item.KategoriId"> @item.KategoriAdi (@item.Makales.Count())</a>
                </li>

            </ul>
        }



    </div>
</div>


